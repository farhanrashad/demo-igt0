<odoo>
  <data>
      
      
       <record id="sale_agreement_form_view_inherit" model="ir.ui.view">
		<field name="name">sale.subscription.agreement.form.view.inherit</field>
		<field name="model">sale.subscription.agreement</field>
		<field name="inherit_id" ref="de_sale_subscription.sale_subcription_agreement_form_view" />
		<field name="arch" type="xml">
          
			<xpath expr="//field[@name='team_id']" position="after">
                <field name="category_id" invisible="0"/> 
                <field name="request_status" string="Approval Status"/> 
                <field name="approval_request_id" invisible="1"/> 
			</xpath>
            
            
           
            <xpath expr="//div[@name='button_box']" position="inside">
                <button name="%(action_approval_request_view_sale_subscription)d" type="action" class="oe_stat_button" icon="fa-check-square-o" string="Approvals" context="{'sale_subscription_id':id,'category_id':category_id}" attrs="{'invisible': [('approval_request_id', '=', False)]}">
                </button>     
            </xpath>
            
		</field>
	</record>
      
  </data>
</odoo>