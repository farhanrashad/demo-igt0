<odoo>
    <data>
        <template id="assets_backend" name="de_attendance_location assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/de_attendance_location/static/src/js/my_attendances.js"></script>
                <link href="/de_attendance_location/static/src/css/google_map.css" rel="stylesheet" type="text/css"/>
            </xpath>
        </template>

        <template id="layout_map" name="Web layout with google map" inherit_id="web.layout">
            <xpath expr="//script" position="before">
                <!--<script t-attf-src="//maps.google.com/maps/api/js?key=#{google_maps_api_key}"></script>-->
                <script t-attf-src="//maps.google.com/maps/api/js?libraries=places&amp;key=#{google_maps_api_key}"></script>
            </xpath>
        </template>

    </data>
</odoo>